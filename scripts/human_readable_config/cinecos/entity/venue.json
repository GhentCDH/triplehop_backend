{
    "data": [
        {
            "system_name": "original_id",
            "display_name": "Original id",
            "type": "Int"
        },
        {
            "system_name": "name",
            "display_name": "Name",
            "type": "String"
        },
        {
            "system_name": "date_opened",
            "display_name": "Date opened",
            "type": "String"
        },
        {
            "system_name": "date_closed",
            "display_name": "Date closed",
            "type": "String"
        },
        {
            "system_name": "status",
            "display_name": "Status",
            "type": "String"
        },
        {
            "system_name": "type",
            "display_name": "Type",
            "type": "String"
        },
        {
            "system_name": "ideological_characteristic",
            "display_name": "Ideological characteristic",
            "type": "String"
        },
        {
            "system_name": "ideological_remark",
            "display_name": "Ideological remark",
            "type": "String"
        },
        {
            "system_name": "infrastructure_info",
            "display_name": "Infrastructure info",
            "type": "String"
        },
        {
            "system_name": "name_remarks",
            "display_name": "Remarks about the name",
            "type": "String"
        },
        {
            "system_name": "screens",
            "display_name": "Number of screens",
            "type": "[String]"
        },
        {
            "system_name": "seats",
            "display_name": "Number of seats",
            "type": "[String]"
        }
    ],
    "display": {
        "title": "$name",
        "layout": [
            {
                "fields": [
                    {
                        "field": "$name"
                    },
                    {
                        "field": "$date_opened",
                        "type": "edtf_year"
                    },
                    {
                        "field": "$date_closed",
                        "type": "edtf_year"
                    },
                    {
                        "field": "$status"
                    },
                    {
                        "field": "$type"
                    },
                    {
                        "field": "$ideological_characteristic"
                    },
                    {
                        "field": "$ideological_remark"
                    },
                    {
                        "field": "$infrastructure_info"
                    },
                    {
                        "field": "$name_remarks"
                    },
                    {
                        "field": "$screens",
                        "type": "list"
                    },
                    {
                        "field": "$seats",
                        "type": "list"
                    }
                ]
            }
        ]
    },
    "es_data": [
        {
            "system_name": "name",
            "display_name": "Name",
            "selector_value": "$name",
            "type": "text"
        },
        {
            "system_name": "city",
            "display_name": "City",
            "base": "$r_venue_address->$r_address_city",
            "parts": {
                "id": {
                    "selector_value": "$r_venue_address->$r_address_city->$id",
                    "type": "integer"
                },
                "name": {
                    "selector_value": "$r_venue_address->$r_address_city->$name",
                    "type": "text"
                }
            },
            "type": "nested"
        },
        {
            "system_name": "street_name",
            "display_name": "Street name",
            "selector_value": "$r_venue_address->$street_name",
            "type": "text"
        },
        {
            "system_name": "date_opened",
            "display_name": "Date opened",
            "selector_value": "$date_opened",
            "type": "edtf",
            "subtype": "start"
        },
        {
            "system_name": "date_closed",
            "display_name": "Date closed",
            "selector_value": "$date_closed",
            "type": "edtf",
            "subtype": "end"
        }
    ],
    "es_display": {
        "title": "Search venues",
        "filters": [
            {
                "filters": [
                    {
                        "filter": "$name",
                        "type": "autocomplete"
                    },
                    {
                        "filter": "$city"
                    },
                    {
                        "filter": "$street_name",
                        "type": "autocomplete"
                    }
                ]
            }
        ],
        "columns": [
            {
                "column": "$name",
                "sortable": true
            },
            {
                "column": "$city",
                "sortable": true
            },
            {
                "column": "$street_name",
                "sortable": true
            },
            {
                "column": "$date_opened",
                "sortable": true
            },
            {
                "column": "$date_closed",
                "sortable": true
            }
        ]
    }
}